// Write A Program to find the sum of the elements in an array.


#include <stdio.h>

#define ARRAY_SIZE(array) (sizeof(array) / sizeof(array[0]))

int main() {
  int array[] = {1, 2, 3, 4, 5};
  int sum = 0;

  for (int i = 0; i < ARRAY_SIZE(array); i++) {
    sum += array[i];
  }

  printf("The sum of the elements in the array is: %d\n", sum);

  return 0;
}
